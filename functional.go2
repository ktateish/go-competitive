func Add[T Numeric](a, b T) T {
	return a + b
}

func Sub[T Numeric](a, b T) T {
	return a - b
}

func Mul[T Numeric](a, b T) T {
	return a * b
}

func Div[T Numeric](a, b T) T {
	return a / b
}

func Rem[T Integer](a, b T) T {
	return a % b
}
